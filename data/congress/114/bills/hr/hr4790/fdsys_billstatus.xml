      <item>
        <titleType>Official Title as Introduced</titleType>
        <title>To promote innovative approaches to outdoor recreation on Federal land and to open up opportunities for collaboration with non-Federal partners, and for other purposes.</title>
        <chamberCode />
        <parentTitleType />
        <chamberName />
      </item>
