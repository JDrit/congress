    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <updateDate>2015-11-19T05:00:00Z</updateDate>
          <text><![CDATA[<p><strong>Preventing Unionization of Revenue Service Employees Act or the PURSE Act </strong></p> <p>This bill excludes employees of the Internal Revenue Service from provisions of federal personnel law allowing federal employees to organize, bargain collectively, and participate in labor organizations. </p>]]></text>
          <name>Introduced in House</name>
          <lastSummaryUpdateDate>2015-12-22T15:22:02Z</lastSummaryUpdateDate>
          <actionDesc>Introduced in House</actionDesc>
          <actionDate>2015-11-19</actionDate>
        </item>
      </billSummaries>
